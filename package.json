    "@lykmapipo/role": "^0.2.0",
    "async": "^2.6.1",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "mongoose-geojson-schemas": "^0.8.1",
