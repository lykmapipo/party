{
  "name": "@lykmapipo/party",
  "version": "0.1.0",
  "description": "A representation of an entity describing information about a particular person or organization, consisting of contact information (e.g. name, e-mail addresses, phone numbers) and other descriptive information of interest.",
  "main": "index.js",
  "scripts": {
    "test": "grunt test",
    "dev": "BASE_PATH=./examples node ./examples/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lykmapipo/party.git"
  },
  "keywords": [
    "lykmapipo",
    "mongoose",
    "party",
    "user",
    "identity",
    "institution",
    "organization",
    "person",
    "people"
  ],
  "contributors": [
    {
      "name": "lykmapipo",
      "email": "lallyelias87@gmail.com",
      "url": "https://github.com/lykmapipo"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lykmapipo/party/issues",
    "email": "lallyelias87@gmail.com"
  },
  "homepage": "https://github.com/lykmapipo/party",
  "sandbox": {
    "url": "https://pa-rty.herokuapp.com",
    "apidoc": "https://lykmapipo.github.io/party/",
    "client_id": "",
    "client_secret": "",
    "urls": [
      {
        "resource": "Party",
        "schema": "https://pa-rty.herokuapp.com/v1/parties/schema",
        "link": "https://pa-rty.herokuapp.com/v1/parties",
        "scopes": [
          "party:*"
        ],
        "methods": [
          "get",
          "post",
          "put",
          "patch"
        ]
      },
      {
        "resource": "Role",
        "schema": "https://pa-rty.herokuapp.com/v1/roles/schema",
        "link": "https://pa-rty.herokuapp.com/v1/roles",
        "scopes": [
          "role:*"
        ],
        "methods": [
          "get",
          "post",
          "put",
          "patch"
        ]
      },
      {
        "resource": "Permission",
        "schema": "https://pa-rty.herokuapp.com/v1/permissions/schema",
        "link": "https://pa-rty.herokuapp.com/v1/permissions",
        "scopes": [
          "permission:*"
        ],
        "methods": [
          "get",
          "put",
          "patch"
        ]
      }
    ]
  },
  "devDependencies": {
    "@benmaruchu/faker": "^4.2.1",
    "@codetanzania/majifix-common": "^0.11.1",
