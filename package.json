    "mongoose-geojson-schemas": ">=0.12.7",
    "mongoose-rest-actions": ">=0.30.4",
    "mongoose-schema-jsonschema": ">=1.4.4"
  },
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=5.6.0"
  }
}
