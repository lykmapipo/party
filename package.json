{
  "name": "@lykmapipo/party",
  "version": "0.1.6",
  "description": "A representation of an entity describing information about a particular person or organization, consisting of contact information (e.g. name, e-mail addresses, phone numbers) and other descriptive information of interest.",
  "main": "index.js",
  "scripts": {
    "test": "grunt test",
    "dev": "BASE_PATH=./examples node ./examples/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lykmapipo/party.git"
  },
  "keywords": [
    "lykmapipo",
    "mongoose",
    "party",
    "user",
    "identity",
    "institution",
    "organization",
    "person",
    "people"
  ],
  "contributors": [
    {
      "name": "lykmapipo",
      "email": "lallyelias87@gmail.com",
      "url": "https://github.com/lykmapipo"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lykmapipo/party/issues",
    "email": "lallyelias87@gmail.com"
  },
  "homepage": "https://github.com/lykmapipo/party",
  "sandbox": {
    "url": "https://pa-rty.herokuapp.com",
    "apidoc": "https://lykmapipo.github.io/party/",
    "client_id": "",
    "client_secret": "",
    "urls": [
      {
        "resource": "Party",
        "schema": "https://pa-rty.herokuapp.com/v1/parties/schema",
        "link": "https://pa-rty.herokuapp.com/v1/parties",
        "scopes": [
          "party:*"
        ],
        "methods": [
          "get",
          "post",
          "put",
          "patch"
        ]
      },
      {
        "resource": "Role",
        "schema": "https://pa-rty.herokuapp.com/v1/roles/schema",
        "link": "https://pa-rty.herokuapp.com/v1/roles",
        "scopes": [
          "role:*"
        ],
        "methods": [
          "get",
          "post",
          "put",
          "patch"
        ]
      },
      {
        "resource": "Permission",
        "schema": "https://pa-rty.herokuapp.com/v1/permissions/schema",
        "link": "https://pa-rty.herokuapp.com/v1/permissions",
        "scopes": [
          "permission:*"
        ],
        "methods": [
          "get",
          "put",
          "patch"
        ]
      }
    ]
  },
  "devDependencies": {
    "@benmaruchu/faker": "^4.2.1",
    "@codetanzania/majifix-common": "^0.12.4",
    "chai": "^4.2.0",
    "grunt": "^1.0.4",
    "grunt-apidoc": "^0.11.0",
    "grunt-contrib-jshint": "^2.1.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-mocha-test": "^0.13.3",
    "handlebars": "^4.1.2",
    "jshint-stylish": "^2.2.1",
    "mocha": "^6.1.4",
    "mongoose": "^5.5.2",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "sinon-mongoose": "^2.3.0",
    "supertest": "^4.0.2"
  },
  "peerDependencies": {
    "mongoose": ">=5.6.4"
  },
  "dependencies": {
    "@lykmapipo/env": ">=0.9.5",
    "@lykmapipo/express-common": ">=0.17.9",
    "@lykmapipo/permission": ">=0.8.5",
    "@lykmapipo/role": ">=0.3.6",
    "async": ">=3.1.0",
    "lodash": ">=4.17.11",
    "moment": ">=2.24.0",
    "moment-timezone": ">=0.5.26",
    "mongoose-geojson-schemas": ">=0.10.8",
    "mongoose-rest-actions": ">=0.28.6",
    "mongoose-schema-jsonschema": ">=1.2.1"
  },
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=5.6.0"
  }
}
